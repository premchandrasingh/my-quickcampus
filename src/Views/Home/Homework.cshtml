@model My.QuickCampus.Models.HomeWorkViewModel
@{
    ViewData["Title"] = "Homework";
    ViewData["CaptureTarget"] = "task-list";
    ViewData["CaptureFileName"] = Model.CaptureFileName;

}

<div id="task-list">
    <div class="container text-center pb-3">
        <div class="row h5">
            <div class="col">
                Homework
            </div>
            <div class="col">
                @Model.StudentName
                
            </div>
            <div class="col">
                @Model.QueryTime
            </div>
        </div>
    </div>

    <partial name="_HomeworkTable" model="Model.Response" />
</div>

